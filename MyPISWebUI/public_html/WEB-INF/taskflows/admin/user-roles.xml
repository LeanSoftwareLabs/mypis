<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="user-roles">
    <default-activity>prepare</default-activity>
    <input-parameter-definition id="__6">
      <name>eventHandler</name>
      <value>#{pageFlowScope.eventHandler}</value>
      <required/>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>userRolesAdminForm</managed-bean-name>
      <managed-bean-class>com.leansoftwarelabs.mypis.view.backing.UserRolesAdminForm</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="user_roles">
      <page>/fragments/admin/user_roles.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <method-call id="prepare">
      <method>#{viewScope.userRolesAdminForm.prepare}</method>
      <outcome>
        <to-string/>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>done</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>prepare</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>continue</from-outcome>
        <to-activity-id>user_roles</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
