<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2" xmlns:security="http://leansoftwarelabs.com/ext/security">
  <task-flow-definition id="baptismal-register-entry">
    <default-activity>gate</default-activity>
    <input-parameter-definition id="__2">
      <name>registerId</name>
      <value>#{pageFlowScope.registerId}</value>
      <required/>
    </input-parameter-definition>
    <input-parameter-definition id="__3">
      <name>eventHandler</name>
      <value>#{pageFlowScope.eventHandler}</value>
      <required/>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>baptismalRegisterEntryForm</managed-bean-name>
      <managed-bean-class>com.leansoftwarelabs.mypis.view.backing.BaptismalRegisterEntryForm</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="baptismal_register_entry">
      <page>/fragments/baptismal_register_entry.jsff</page>
    </view>
    <router id="gate">
      <case id="__8">
        <expression>#{pageFlowScope.registerId eq -1}</expression>
        <outcome>prepare</outcome>
      </case>
      <default-outcome>toEntry</default-outcome>
    </router>
    <method-call id="prepareEditing">
      <method>#{viewScope.baptismalRegisterEntryForm.edit}</method>
      <outcome>
        <fixed-outcome>toEntry</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__4">
      <from-activity-id>gate</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>toEntry</from-outcome>
        <to-activity-id>baptismal_register_entry</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>prepare</from-outcome>
        <to-activity-id>prepareEditing</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>prepareEditing</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>toEntry</from-outcome>
        <to-activity-id>baptismal_register_entry</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>done</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
