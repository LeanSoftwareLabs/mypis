<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="contact-list">
    <default-activity>contact_list</default-activity>
    <input-parameter-definition id="__2">
      <name>eventHandler</name>
      <value>#{pageFlowScope.eventHandler}</value>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>contactListForm</managed-bean-name>
      <managed-bean-class>com.leansoftwarelabs.mypis.view.backing.ContactListForm</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="contact_list">
      <page>/fragments/acctg/contact_list.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
